<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FocusImageView">
        <attr name="focus_focusing_id" format="integer" />
        <attr name="focus_success_id" format="integer" />
        <attr name="focus_fail_id" format="integer" />
    </declare-styleable>

    <declare-styleable name="TitleBar">
        <attr name="back_image" format="reference"/>
        <attr name="title_text" format="string|reference"/>
        <attr name="bg_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="FastSettingArrowsItemView">
        <attr name="item_name" format="string|reference"/>
        <attr name="right_icon" format="reference"/>
        <attr name="right_icon_visibility" format="enum">
            <enum name="visible" value="0"/>
            <enum name="gone" value="1"/>
            <enum name="invisible" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="FastSettingRadioItemView">
        <attr name="radio_item_name" format="string|reference"/>
        <attr name="radio_right_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="Grid">
        <attr name="with_diagonal" format="boolean"/>
    </declare-styleable>
</resources>